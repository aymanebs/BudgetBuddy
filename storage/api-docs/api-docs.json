                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "description",
                                    "amount",
                                    "category"
                                ],
                                "properties": {
                                    "description": {
                                        "type": "string",
                                        "format": "string",
                                        "example": "Electricity Bill"
                                    },
                                    "amount": {
                                        "type": "integer",
                                        "format": "integer",
                                        "example": "5000"
                                    },
                                    "category": {
                                        "type": "string",
